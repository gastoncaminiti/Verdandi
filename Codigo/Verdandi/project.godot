; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Verdandi"
run/main_scene="res://interfaces/LogoInterface.tscn"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

CardConfig="*res://code/manager/card/CardConfigManager.gd"
CardEngine="*res://code/manager/card/CardEngineManager.gd"
CardGame="*res://code/manager/card/CardGameManager.gd"
DataManager="*res://code/manager/DataManager.gd"

[debug]

gdscript/warnings/enable=false
shapes/collision/shape_color=Color( 0, 0.6, 0.701961, 0.501961 )

[display]

window/size/width=640
window/size/height=360
window/size/fullscreen=true
window/size/test_width=1280
window/size/test_height=720
window/per_pixel_transparency/enabled=true
window/handheld/orientation="reverse_portrait"
mouse_cursor/custom_image="res://interfaces/norse/img/cursor1.5x.png"
mouse_cursor/tooltip_position_offset=Vector2( 0, 0 )
window/stretch/mode="2d"
window/stretch/aspect="keep"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": true,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[locale]

translations=PoolStringArray( "res://localization/translation/Interfaces.en.translation", "res://localization/translation/Interfaces.es.translation", "res://localization/translation/Runes.en.translation", "res://localization/translation/Runes.es.translation", "res://localization/translation/Milestones.en.translation", "res://localization/translation/Milestones.es.translation" )

[rendering]

quality/2d/use_pixel_snap=true
